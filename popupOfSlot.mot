function getUserBalancePromise(userID) {

  return new Promise((resolve, reject) =&gt; {
    ExecuteQueryModified(&apos;SELECT `Balance` FROM `APPLICATION_636` where id=&apos; + userID, Grid_1_callbackFunc);

    function Grid_1_callbackFunc(error, xhr) {
      if (error) {
        reject(error)
      } else if (xhr.response) {
        Grid_1DataArr = JSON.parse(xhr.response);
        resolve(Grid_1DataArr[0].Balance);
        //console.log(&quot;Balance----&quot;, Grid_1DataArr[0].Balance) /////////////debugging
        flagGrid_1_callbackFunc = 0;

      }
    }

  });
}
